<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <%= @event.description %>
    </div>
  </div>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <%= @event.notes %>
    </div>
  </div>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <%= @event.link %>
    </div>
  </div>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-6">Start Time</div>
    <div class="col-md-6">
      <%= @event.start_time %>
