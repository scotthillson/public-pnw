<%= link_to 'New Image', new_image_path, class: 'btn btn-primary' %>
<br>
<% @images.each do |image| %>
  <%= link_to 'delete', image, method: :delete, data: { confirm: ' Are you sure you want to destroy this image?' }, class: 'btn btn-danger btn-sm' %>
  <br>
  <img src="<%= image.filename %>" alt="image">
  <br>
<% end %>
