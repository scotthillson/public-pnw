<table class="table">
  <thead>
    <tr>
      <th class="text-center">Name</th>
      <th class="text-center">Email</th>
      <th class="text-center">Created</th>
    </tr>
  </thead>
  <tbody>
    <% @applications.each do |a| %>
      <tr class="text-center">
        <td><%= a.name %></td>
        <td><%= a.email %></td>
        <td><%= a.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
